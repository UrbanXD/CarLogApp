{
  "expo": {
    "name": "Taxi",
    "slug": "Taxi",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "taxi",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "translucent": true
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.urbanxd.Taxi"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Nosifer.otf",
            "./src/assets/fonts/Gilroy-Heavy.otf",
            "./src/assets/fonts/Gilroy-Regular.ttf",
            "./src/assets/fonts/Gilroy-Medium.ttf",
            "./src/assets/fonts/DoubleFeature20.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "84e8549c-e28c-426d-b5d5-8a1cc83c8da9"
      }
    }
  }
}
