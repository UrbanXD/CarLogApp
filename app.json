{
  "expo": {
    "name": "Carlog",
    "slug": "Taxi",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "carlog",
    "icon": "./src/assets/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "translucent": true
    },
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./src/assets/ios-dark.png",
        "light": "./src/assets/ios-light.png",
        "tinted": "./src/assets/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "monochromeImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.urbanxd.Carlog"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/C.png",
          "resizeMode": "contain",
          "imageWidth": 74,
          "backgroundColor": "#242424"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "2.0.21"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Default",
            "enforceNavigationBarContrast": false
          }
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "84e8549c-e28c-426d-b5d5-8a1cc83c8da9"
      }
    }
  }
}
